<odoo>
  <data>
    <record model="ir.ui.view" id="agf.usd_rate_list">
      <field name="name">agf_year_rate list</field>
      <field name="model">agf.usdrate</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="rate"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="agf.usdrate_action_window">
      <field name="name">Rate for Current Year</field>
      <field name="res_model">agf.usdrate</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>